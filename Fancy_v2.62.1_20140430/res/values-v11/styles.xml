<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Holo.ListView.Base" parent="@android:style/Widget.Holo.ListView" />
    <style name="Holo.Theme" parent="@style/Holo.Base.Theme">
        <item name="android:actionBarWidgetTheme">@style/Holo.Theme</item>
    </style>
    <style name="Holo.Theme.Dialog" parent="@style/Holo.Base.Theme.Dialog">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
    <style name="Holo.Theme.Dialog.Light" parent="@style/Holo.Base.Theme.Dialog.Light">
