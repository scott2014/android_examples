<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yhj.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="10" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.yhj.app.AppStart"
            android:label="@string/title_activity_app_start"
            android:theme="@style/AnimStyle2" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.yhj.app.Welcome"
            android:label="@string/title_activity_welcome"
            android:theme="@style/AnimStyle2" >
        </activity>
        <activity
            android:name="com.yhj.app.Login"
            android:label="@string/title_activity_login"
            android:theme="@style/AnimStyle2" >
        </activity>
        <activity
            android:name="com.yhj.app.Loading"
            android:label="@string/title_activity_loading"
            android:theme="@style/MyDialogStyle" >
        </activity>
        <activity
            android:name="com.yhj.app.Whatsnew"
            android:label="@string/title_activity_whatsnew"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.yhj.app.WhatsnewDoor"
            android:label="@string/title_activity_whatsnew_door"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.yhj.app.MainWeiXin"
            android:label="@string/title_activity_main_wei_xin"
            android:theme="@android:style/Theme.NoTitleBar" >
        </activity>
        <activity
            android:name="com.yhj.app.WeixinTopDialog"
            android:label="@string/title_activity_weixin_top_dialog"
            android:theme="@style/MyDialogStyle2" >
        </activity>
        <activity
            android:name="com.yhj.app.ExitFromSetting"
            android:label="@string/title_activity_exit_from_setting"
            android:theme="@style/MyDialogStyleBottom">
        </activity>
    </application>

</manifest>
